{
  "name": "revolution/authorize-manager",
  "description": "Authorize HTTP Client Manager for Laravel",
  "keywords": [
    "authorize",
    "HTTP client",
    "guzzle",
    "goutte",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2||^8.0",
    "guzzlehttp/guzzle": "^6.3||^7.0",
    "fabpot/goutte": "^3.3",
    "illuminate/support": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0||^9.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Authorize\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Authorize\\Providers\\AuthorizeServiceProvider"
      ],
      "aliases": {
        "Authorize": "Revolution\\Authorize\\Facades\\Authorize"
      }
    }
  }
}
