{
  "name": "revolution/authorize",
  "description": "Authorize HTTP Client Manager for Laravel",
  "keywords": [
    "HTTP client",
    "guzzle",
    "goutte",
    "laravel"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "guzzlehttp/guzzle": "^6.3",
    "fabpot/goutte": "^3.2",
    "illuminate/support": "^5.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Revolution\\Authorize\\": "src/"
    }
  },
  "authors": [
    {
      "name": "kawax",
      "email": "kawaxbiz@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Revolution\\Authorize\\Providers\\AuthorizeServiceProvider"
      ],
      "aliases": {
        "Authorize": "Revolution\\Authorize\\Facades\\Authorize"
      }
    }
  }
}
